{
"$schema": "http://json-schema.org/draft-07/schema#",
"$id": "http://example.com/example.json",
"type": "object",
"required": ["title", "companyHandle"],
"properties": {
    "title": {
        "type": "string"     
    },
    "salary": {
        "type": "integer",        
        "minLength": 0
    },
    "equity": {
        "type": "integer",
        "minLength": 0,
        "maxLength": 1
    },
    "companyHandle": {
        "type": "string"
    }
},
"additionalProperties": false
}
